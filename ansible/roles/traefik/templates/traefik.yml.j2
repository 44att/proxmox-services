---
entryPoints:
  web:
    address: :80

  websecure:
    address: :443

providers:
  file:
    directory: /etc/traefik/config
    watch: true

api:
  dashboard: true
  insecure: true

certificatesResolvers:
  letsencrypt:
    acme:
      email: "{{ acme_email }}"
      storage: /traefik-config/acme.json
      dnsChallenge:
        provider: cloudflare
        resolvers:
          - "9.9.9.9"
          - "149.112.112.112"